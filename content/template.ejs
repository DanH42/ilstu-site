<html class="home no-js standard" lang="en">
<!--
<![endif]-->
	<head>
		<meta charset="utf-8" />
		<title><%= $.title %></title>
		<link rel="shortcut icon" href="/~dhlaven/favicon.ico" type="image/x-icon" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
		<link id="main_css" href="/~dhlaven/css/style.css" rel="stylesheet" />
		<script>
		window.jQuery || document.write('<script src="/~dhlaven/scripts/libs/jquery-1.7.1.min.js"><\/script>');
		(function(){
			var cx = '014430627749204945988:9zenr1ceewi';
			var gcse = document.createElement('script');
			gcse.type = 'text/javascript';
			gcse.async = true;
			gcse.src = 'http://www.google.com/cse/cse.js?cx=' + cx;
			var s = document.getElementsByTagName('script')[0];
			s.parentNode.insertBefore(gcse, s);
		})();
		</script>
		<script src="/~dhlaven/scripts/libs/modernizr-2.5.3.min.js"></script>
		<% for(var i = 0; i < $.styles.length; i++){ %>
			<link rel="stylesheet" type="text/css" href="<%= $.styles[i] %>" />
		<% } %>
	</head>
	<body>
		<div id="container">
			<header>
				<section id="mastleft">
					<div id="mastlogo">
						<a class="linkhome" href="/~dhlaven/">
							<img src="/~dhlaven/img/logo/ISU-seal-Dan-Hlavenka-wide.png" alt="Dan Hlavenka - Illinois State University" width="362" height="80" />
							<span class="visuallyhidden">Dan Hlavenka - Illinois State University</span>
						</a>
					</div>
					<div id="mastlogoprint">
						<img src="/~dhlaven/img/logo/wordmark_print.jpg" alt="Illinois State University" />
					</div>
					<div id="mastlogoshare">
						<img src="/~dhlaven/img/logo/seal_red.jpg" alt="Illinois State University" />
					</div>
				</section>
				<section id="mastright">
					<div id="searchbox">
						<form id="searchform" action="/~dhlaven/Search/" method="get">
							<div id="searchbackground" class="searchbackground_active">
								<label for="searchboxtext" class="searchboxlabel">Search</label>
								<input id="searchboxtext" class="searchboxtext_active" type="text" name="q" />
								<!--<input id="searchbutton" title="Search" type="submit" alt="Search" />-->
							</div>
						</form>
						<script type="text/javascript">
						var enableAutoComplete = function(){
							google.search.CustomSearchControl.attachAutoCompletionWithOptions(
								'014430627749204945988:9zenr1ceewi',
								document.getElementById('searchboxtext'),
								'searchform');
						};

						window.__gcse = {
							callback: function(){
								if(document.readyState == 'complete')
									enableAutoComplete();
								else
									google.setOnLoadCallback(enableAutoComplete, true);
							}
						};
						</script>
						<script type="text/javascript" src="//www.google.com/cse/brand?form=searchform&inputbox=searchboxtext"></script>
						<% if($.name == "Search"){ %>
							<script type="text/javascript">
							var searches = window.location.search.split(/[\?\&]q=/);
							if(searches.length !== 0)
								document.getElementById('searchboxtext').value = searches[1].split("&")[0].replace(/\+/g, " ");
							</script>
						<% } %>
					</div>
				</section>
			</header>
			<nav id="mastnav">
				<span class="mastnav_top"></span>
				<span class="mastlevel1">
					<span class="mastlevel1_top"></span>
					<span class="mastlink1 <%= $.home %>">
						<span class="mastlink_top"></span>
						<span class="mastlink_mid">
							<a class="<%= $.home %> home" href="/~dhlaven/">Home</a>
						</span>
						<span class="mastlink_btm"></span>
					</span>
					<% for(var i = 0; i < $.tabs.length; i++){ %>
						<span class="mastlink2 <%= $.tabs[i].current %>">
							<span class="mastlink_top"></span>
							<span class="mastlink_mid">
								<a class="<%= $.tabs[i].current %>" href="<%= $.tabs[i].link %>"><%= $.tabs[i].name %></a>
							</span>
							<span class="mastlink_btm"></span>
						</span>
					<% } %>
					<span class="mastlevel1_btm"></span>
				</span>
				<span class="mastnav_btm"></span>
			</nav>
			<% if($.links){ %>
				<nav id="subnav">	 	
					<span>Find me on...</span>	 	
					<ul>
					<% for(var i = 0; i < $.links.length; i++){ %>
						<li><a href="<%= $.links[i].link %>"><%= $.links[i].name %></a></li>
					<% } %>
					</ul>
				</nav>
			<% } %>
			<div id="body" class="basic sectionnavon">
				<span class="body_top"></span>
				<div id="wrapcontent">
					<section class="content1" id="section2">
						<span class="section_top"></span>
						<% if($.name == "Resume"){ %>
						<div id="content">
						<% }else{ %>
						<div class="section_mid">
						<% } %>
							<%- $.content %>
						</div>
						<span class="section_btm"></span>
					</section>
				</div>
				<span class="body_btm"></span>
			</div>
			<footer>
				<div id="superfooter">
					<img style="border: 0px;" src="/~dhlaven/img/footerIcon.png" alt="" />
				</div>
				<div id="footerdepartment">
					<p>
						1718 Hewett Hall<br />
						101 N Fell Ave<br />
						Normal, Il 61761<br />

						<a href="mailto:dhlavenka@ilstu.edu">dhlavenka@ilstu.edu</a>
					</p>
				</div>
				<div id="footeruniversity">
					<p>
						<span id="ou_de">©</span> <span id="c_year">2013</span>
						<a href="http://IllinoisState.edu">Dan Hlavenka / Illinois State University</a><br />
						<span class="noprint">
							<a href="http://IllinoisState.edu/diversity/">An equal opportunity/affirmative action university encouraging diversity.</a><br />

							<a href="http://IllinoisState.edu/downloads/web-privacy-statement-2010.pdf">Privacy Statement</a>
							•
							<a href="http://universitymarketing.illinoisstate.edu/identity/">Identity Standards</a>
							•
							<a href="http://www.policy.ilstu.edu/technology/9-2.shtml">Appropriate Use Policy</a>
						</span>
					</p>
				</div>
			</footer>

			<!--[if lt IE 7 ]>
				<script src="//ajax.googleapis.com/ajax/libs/chrome-frame/1.0.2/CFInstall.min.js"></script>
				<script>
				window.attachEvent("onload",function(){
					CFInstall.check({mode:"overlay"});
				});
				</script>
			<![endif]-->
		</div>

		<script type="text/javascript">
		var _paq = _paq || [];
		_paq.push(["trackPageView"]);
		_paq.push(["enableLinkTracking"]);
		(function(){
			var url = (("https:" == document.location.protocol) ? "https" : "http") + "://track.xd6.co/";
			_paq.push(["setTrackerUrl", url + "piwik.php"]);
			_paq.push(["setSiteId", "5"]);
			var g = document.createElement("script");
			var s = document.getElementsByTagName("script")[0];
			g.type = "text/javascript";
			g.defer = true;
			g.async = true;
			g.src = url + "piwik.js";
			s.parentNode.insertBefore(g, s);
		})();
		</script>
	</body>
</html>
